<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" >
  <head>
    <title>Sassijs Tests</title>    
    <script language="javascript" src="../jquery-1.3.1.min.js" type="text/javascript"></script>
    <script language="javascript" src="../testrunner.js" type="text/javascript"></script>
    <script language="javascript" src="../testnavigation.js" type="text/javascript"></script>
    <link media="screen" href="../testsuite.css" type="text/css" rel="stylesheet"/>    
    <script language="javascript" src="../testsetup.js" type="text/javascript"></script>
  </head>
  <body>
    <script language="javascript" type="text/javascript">

  module( "Sassijs Expression Tests");
  
  test( "should find sub-expressions inside parenthesis for order-of-precedence", function(){
    sassijsExpression = new SassijsExpression( "=(10 + 15) + px" );
    same( sassijsExpression.getFirstPrecedentExpression(), [ "=", "10 + 15", " + px" ] );
    sassijsExpression = new SassijsExpression( "=(10 + ( 3 * 5 ) ) + px" );
    same( sassijsExpression.getFirstPrecedentExpression(), [ "=", "10 + ( 3 * 5 ) ", " + px" ] );
    sassijsExpression = new SassijsExpression( "=(10 + 3 ) * ( 10 - 5 ) + px" );
    same( sassijsExpression.getFirstPrecedentExpression(), [ "=", "10 + 3 ", " * ( 10 - 5 ) + px" ] );
  });
/*
  test( "should take a string representation of an expression and evaluate it", function(){
    sassijsExpression = new SassijsExpression( "=(10 + 15) + px" );
    equals( sassijsExpression.getCss(), '35px' );
  });
*/  
    </script>
    <h1></h1>
    <h2 id="banner">
      <select id='test_select'></select>
    </h2>
    <ol id="tests"></ol>
    <div id="results"></div>
    <div id="main"></div>

  </body>
</html>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" >
  <head>
    <title>Sassijs Tests</title>    
    <script language="javascript" src="../../jquery-1.3.1.min.js" type="text/javascript"></script>
    <script language="javascript" src="../../testrunner.js" type="text/javascript"></script>
    <script language="javascript" src="../../testnavigation.js" type="text/javascript"></script>
    <link media="screen" href="../../testsuite.css" type="text/css" rel="stylesheet"/>    
    <script language="javascript" src="../../testsetup.js" type="text/javascript"></script>
  </head>
  <body>
    <script language="javascript" type="text/javascript">

  module( "Sassijs Expression Operation Tests");
 
  test( "SassijsExpressionOperation should evaluate to a value", function(){
    sassijsExpressionOperation = new SassijsExpressionOperation( '2', '+', '3' );
    equals( sassijsExpressionOperation.getValue(), '5' );

    sassijsExpressionOperation = new SassijsExpressionOperation( 'blue', '+', '5px' );
    equals( sassijsExpressionOperation.getValue(), 'blue5px' );
  });
 
          
    </script>
    <h1></h1>
    <h2 id="banner">
      <select id='test_select'></select>
    </h2>
    <ol id="tests"></ol>
    <div id="results"></div>
    <div id="main"></div>
 
  </body>
</html>
